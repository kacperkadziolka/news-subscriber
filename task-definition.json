{
  "family": "ecrrepositorytest",
  "containerDefinitions": [
    {
      "image": "REPOSITORY_URI:BUILD_NUMBER",
      "name": "ecrrepositorytest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::047645927285:role/ecsTaskExecutionRole"
}